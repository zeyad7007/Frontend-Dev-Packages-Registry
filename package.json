{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:e2e": "tsc -p tsconfig.e2e.json",
    "build:unit": "tsc -p tsconfig.unit.json",
    "build:all": "tsc -b",
    "test:e2e": "npm run build:e2e && node dist/e2e/your-e2e-test-file.js",
    "test:unit": "npm run build:unit && node dist/unit/your-unit-test-file.js",
    "test:all": "npm run build:all && npm run run:all-tests",
    "run:all-tests": "node runAllTests.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/axios": "^0.14.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}
