{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "include": ["src/**/*.{ts,tsx}"],
  "exclude": [
    "node_modules",
    "tests",
    "coverage",
    "vite.config.ts",
    "src/Interface.ts",
    "src/vite-env.d.ts"
  ],
  "reporter": ["text", "html", "json", "lcov"],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": true,
  "statements": 40,
  "branches": 40,
  "functions": 40,
  "lines": 40
}
